    "vue-router": "^4.5.1",
    "node-fetch": "^3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
