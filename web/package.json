{
  "name": "ccdebug-web",
  "version": "1.0.0",
  "description": "Web timeline interface for Claude Code Debug",
  "type": "module",
  "files": [
    "dist/**/*",
    "server/**/*",
    "public/**/*",
    "index.html",
    "vite.config.*",
    "tsconfig.*",
    "!node_modules",
    "!src",
